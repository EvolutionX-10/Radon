{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"experimentalDecorators": true,
		"allowJs": true,
		"outDir": "../dist",
		"rootDir": ".",
		"baseUrl": ".",
		"paths": {
			"#constants": ["lib/utility/constants.ts"],
			"#config": ["lib/utility/config.ts"],
			"#lib/*": ["lib/*"],
			"#root/*": ["*"]
		},
		"noEmit": true,
		"skipLibCheck": true,
		"moduleResolution": "NodeNext",
		"module": "NodeNext"
	},
	"include": ["."]
}
