{
    "name": "radon",
    "version": "0.1.2",
    "description": "An advanced moderation discord bot",
    "main": "./dist/index.js",
    "imports": {
        "#lib/RadonClient": "./dist/lib/RadonClient.js",
        "#lib/types": "./dist/lib/types/index.js",
        "#lib/structures": "./dist/lib/structures/index.js",
        "#lib/setup": "./dist/lib/setup/index.js",
        "#lib/utility": "./dist/lib/utility/index.js",
        "#lib/*": "./dist/lib/*/index.js",
        "#models": "./dist/models/index.js",
        "#vars": "./dist/vars.js",
        "#config": "./dist/config.js"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "yarn compile && NODE_ENV=production && node ./dist/index.js",
        "dev": "yarn build && node ./dist/index.js",
        "release": "standard-version && git push --follow-tags",
        "preview": "standard-version --dry-run",
        "compile": "tsc -p ./src",
        "watch": "tsc -b src --watch"
    },
    "engines": {
        "node": ">=17.4.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EvolutionX-10/Radon.git"
    },
    "keywords": [
        "discord",
        "bot",
        "discord.js",
        "moderation"
    ],
    "author": "EvolutionX",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/EvolutionX-10/Radon/issues"
    },
    "homepage": "https://github.com/EvolutionX-10/Radon#readme",
    "devDependencies": {
        "@types/figlet": "^1.5.4",
        "@types/gradient-string": "^1.1.2",
        "@types/humanize-duration": "^3.27.1",
        "@types/ms": "^0.7.31",
        "@types/node": "^17.0.21",
        "@types/string-similarity": "^4.0.0",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.10.0",
        "prettier": "^2.5.1",
        "standard-version": "^9.3.2",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@sapphire/decorators": "^4.2.5",
        "@sapphire/discord-utilities": "^2.8.0",
        "@sapphire/discord.js-utilities": "^4.8.1",
        "@sapphire/framework": "next",
        "@sapphire/pieces": "^3.2.0",
        "@sapphire/plugin-editable-commands": "^1.1.3",
        "@sapphire/plugin-hmr": "^1.1.1",
        "@sapphire/plugin-logger": "^2.1.3",
        "@sapphire/plugin-subcommands": "^2.1.3",
        "@sapphire/stopwatch": "^1.3.0",
        "@sapphire/time-utilities": "^1.6.0",
        "@sapphire/ts-config": "^3.3.1",
        "@sapphire/type": "^2.1.2",
        "@sapphire/utilities": "^3.3.0",
        "axios": "^0.26.0",
        "colorette": "^2.0.16",
        "discord.js": "^13.6.0",
        "dotenv-cra": "^3.0.2",
        "figlet": "^1.5.2",
        "gradient-string": "^2.0.0",
        "humanize-duration": "^3.27.1",
        "mongoose": "^6.2.3",
        "ts-node": "^10.5.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "packageManager": "yarn@3.2.0"
}
